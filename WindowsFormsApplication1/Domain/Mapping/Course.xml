<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Domain"
                   namespace="Domain">
<class name="Course" table="Course">
    <id name="id" column="Course_id" type="int">
      <generator class="native"></generator>
    </id>
  <property name="date" column="Course_date" not-null="true"/>
  <property name="distance" column="Course_distance" not-null="true"/>
  <property name="lieu" column="Course_lieu" not-null="true"/>
  <property name="description" column="Course_desc" not-null="true"/>
  </class>
</hibernate-mapping>