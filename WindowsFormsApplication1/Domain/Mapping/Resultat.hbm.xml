<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Domain"
                   namespace="Domain">
  <class name="Resultat" table="Resultat">
    <id name="Id" column="resultat_Id" type="int">
      <generator class="native"></generator>
    </id>
    <property name="Temps" column="resultat_temps" not-null="true"/>
    <property name="VitesseMoyenne" column="resultat_vitMoy" not-null="true"/>
    <property name="AllureMoyenne" column="resultat_AllureMoy" not-null="true"/>
    <property name="LaCourse" column="resultat_Course" not-null="true"/>
    <property name="LeParticipant" column="resultat_Participant" not-null="true"/>
  </class>
</hibernate-mapping>
