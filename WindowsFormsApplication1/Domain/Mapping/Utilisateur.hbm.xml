<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Domain"
                   namespace="Domain">
  <class name="Utilisateur" table="Utilisateur">
    <id name="Id" column="utilisateur_id" type="int">
      <generator class="native"></generator>
    </id>
    <property name="Pseudo" column="utilisateur_pseudo" not-null="true"/>
    <property name="MotDePasse" column="utilisateur_mdp" not-null="true"/>
  </class>
</hibernate-mapping>
