<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Domain"
                   namespace="Domain">
  <class name="Participant" table="Participant">
    <id name="NumLicence" column="participant_numLicence" type="int">
      <generator class="native"></generator>
    </id>
    <property name="Prenom" column="participant_Nom" not-null="true"/>
    <property name="Nom" column="participant_Prenom" not-null="true"/>
    <property name="Sexe" column="participant_Sexe" not-null="false"/>
    <property name="DateDeNaissance" column="participant_Date_Naissance" not-null="false"/>
    <property name="Courriel" column="participant_Courriel" not-null="true"/>
    <property name="LesResultats" column="participant_Resultats" not-null="false"/>
  </class>
</hibernate-mapping>


